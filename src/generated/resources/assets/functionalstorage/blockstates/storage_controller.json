      }
    },
    {
      "apply": {
        "model": "functionalstorage:block/storage_controller",
        "uvlock": true,
        "y": 270
      },
      "when": {
        "subfacing": "west"
      }
    },
    {
      "apply": {
        "model": "functionalstorage:block/storage_controller",
        "uvlock": true,
        "y": 90
      },
      "when": {
        "subfacing": "east"
      }
    }
  ]
}